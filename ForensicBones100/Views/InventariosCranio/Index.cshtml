@model IEnumerable<ForensicBones100.Models.InventarioCranio>

@{
    ViewData["Title"] = "Index";
}

<div class="tituloPage m-3 text-center">
    <h4><b>RELATÓRIO DE ANTROPOLOGIA FORENSE - INVENTÁRIO DO CRÂNIO</b></h4>

</div>
<hr />
<div class="btn-card mb-3 ms-5 mx-auto">
    <a asp-action="Create" class="btn custom-button-card">Novo Inventário</a>
</div>

<div class="container pb-3">
    @foreach (var item in Model)
    {
        <div class="card mb-3">
            <div class="card-header text-center">
                <h5 class="card-title" id="">Relatório: <b>@Html.DisplayFor(modelItem => @item.Relatorio.Codigo)</b></h5>
            </div>
            <div class="card-body text-center">

                <h5 class="card-title mt-3 pb-5">Fotos do Crânio</h5>
                <p class="card-text">
                    <img src="@item.FotosCranio" alt="Imagem" class="img-thumbnail d-flex mx-auto" style="max-width: 100%;" />
                    Foto: @Html.DisplayFor(modelItem => item.FotosCranio)
                </p>
                <hr />
                <h5 class="card-title mb-3">Ossos</h5>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Frontal: <b>@item.Frontal</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Frontal:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.FrontalDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Ocipital:  <b>@item.Ocipital</b></label>
                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Ocipital:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.OcipitalDesc</textarea>
                </div>
                <hr />

                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Esfenoide:  <b>@item.Esfenoide</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Esfenoide:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.EsfenoideDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Maxilar: <b>@item.Maxilar</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Maxilar:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.MaxilarDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Vomer: <b>@item.Vomer</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Vomer:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.VomerDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Parietal Esquerdo: <b>@item.ParietalEsquerdo</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Parietal Esquerdo:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.ParietalEsquerdoDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Temporal Esquerdo: <b>@item.TemporalEsquerdo</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Temporal Esquerdo:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.TemporalEsquerdoDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Concha Nasal Esquerda: <b>@item.ConchaNasalInferiorEsquerda</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Concha Nasal Esquerda:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.ConchaNasalInferiorEsquerdaDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Etmoide Esquerdo: <b>@item.Etmoide</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Etmoide Esquerdo:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.EtmoideDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Lacrimal Esquerdo: <b>@item.LacrimalEsquerdo</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Lacrimal Esquerdo:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.LacrimalEsquerdoDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Nasal Esquerdo: <b>@item.NasalEsquerdo</b></label>

                </div>
                <div class="Observacoes align-content-start  m-1">
                    <label for="Observacoes" class="col-sm-4 mb-3 mx-auto">Descrição Etmoide Esquerdo:</label>
                    <textarea for="Observacoes" name="Observacoes" class="form-control text-center" readonly>@item.NasalEsquerdoDesc</textarea>
                </div>
                <hr />
                <div class="form-group mb-3 col-8 mx-auto">
                    <label for="Relatorio" class="col-sm-4 mb-3 mx-auto">Zigomatico Esquerdo: <b>@item.ZigomaticoEsquerdo</b></label>
                </div>
